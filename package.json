{
  "name": "zolfinak",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "An Ionic project",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "fmt": "prettier --write '**/*.{ts,tsx}'",
    "android.list": "ionic cap run android --list",
    "android.open": "ionic cap open android",
    "android.build": "ionic cap sync && ionic cap copy",
    "android.start.only": "ionic cap run android --external",
    "android.start": "nr android.build && nr android.start.only",
    "android.log": "adb shell \"logcat\" | grep -i 'Capacitor' | awk '{ for (i=5; i <= NF; i++) printf FS$i; print NL }'",
    "android.log-long": "adb shell \"logcat\" | grep -i 'Capacitor'",
    "build.full": "nr build && ionic build && nr android.build"
  },
